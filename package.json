{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "npm run create-placeholders && next build",
    "create-placeholders": "node ./scripts/create-placeholders.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "npm run create-placeholders && next dev",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,scss,md}\"",
    "lint:scss": "stylelint '**/*.scss'",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "majestic": "EDITOR=code majestic",
    "posttest": "npm run format",
    "prepare": "husky install",
    "pretest": "npm run lint:scss && npm run lint",
    "start": "next start",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:mutate": "npx stryker run",
    "test:mutate-file": "npx stryker run --concurrency 1 --reporters clear-text,progress-append-only,html --mutate \"**/$FILE_NAME*.tsx,!**/*@(.data|.test|.spec|Spec|.stories).ts?(x)\""
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@mapbox/rehype-prism": "^0.8.0",
    "canvas": "2.9.0",
    "gray-matter": "^4.0.3",
    "next": "12.1.0",
    "next-themes": "^0.1.1",
    "postcss": "^8.4.12",
    "preact": "^10.6.6",
    "prismjs": "^1.27.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-highlight": "^5.0.2",
    "rehype-parse": "^8.0.4",
    "rehype-react": "^7.0.4",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "sass": "^1.49.9",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@plaiceholder/base64": "^1.0.0",
    "@stryker-mutator/core": "^5.6.1",
    "@stryker-mutator/jest-runner": "^5.6.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "axe-core": "^4.4.1",
    "cypress": "^9.5.1",
    "cypress-axe": "^0.14.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "eslint-config-jsdoc": "^15.4.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "image-size": "^1.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "majestic": "^1.8.1",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.5.1",
    "sharp": "^0.30.2",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests"
    ],
    "*.scss": "stylelint --fix",
    "*.{ts,tsx,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "17.0.43"
    }
  }
}
