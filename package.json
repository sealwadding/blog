{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "npm run create-placeholders && next build",
    "create-placeholders": "node ./scripts/create-placeholders.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "npm run create-placeholders && next dev",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,scss,md}\"",
    "lint:scss": "stylelint '**/*.scss'",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "majestic": "EDITOR=code majestic",
    "posttest": "npm run format",
    "prepare": "husky install",
    "pretest": "npm run lint:scss && npm run lint",
    "start": "next start",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:mutate": "npx stryker run",
    "test:mutate-file": "npx stryker run --concurrency 1 --reporters clear-text,progress-append-only,html --mutate \"**/$FILE_NAME*.tsx,!**/*@(.data|.test|.spec|Spec|.stories).ts?(x)\""
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.6.0",
    "gray-matter": "^4.0.3",
    "next": "11.0.0",
    "next-themes": "^0.0.14",
    "preact": "^10.5.13",
    "prismjs": "^1.23.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rehype-highlight": "^4.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-react": "^6.2.0",
    "rehype-stringify": "^8.0.0",
    "remark": "^13.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "sass": "^1.35.1",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.0.0",
    "@plaiceholder/base64": "^1.0.0",
    "@stryker-mutator/core": "^5.1.1",
    "@stryker-mutator/jest-runner": "^5.1.1",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "axe-core": "^4.2.2",
    "cypress": "^7.5.0",
    "cypress-axe": "^0.12.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-jsdoc": "^15.4.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "image-size": "^1.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "majestic": "^1.8.0",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.3.1",
    "sharp": "^0.28.3",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "jest --bail --findRelatedTests"
    ],
    "*.scss": "stylelint --fix",
    "*.{ts,tsx,scss,md}": "prettier --write"
  }
}
